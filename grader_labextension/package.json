{
  "name": "grader-labextension",
  "version": "0.1.0",
  "description": "A JupyterLab extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/github_username/grader-labextension",
  "bugs": {
    "url": "https://github.com/github_username/grader-labextension/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Elias Wimmer, Florian JÃ¤ger & Matthias Matt",
    "email": ""
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/TU-Wien-dataLAB/Grader-Service.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "npm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf grader_labextension/labextension",
    "clean:all": "npm run clean:lib && npm run clean:labextension && npm run clean:lintcache",
    "eslint": "npm run eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "npm run build",
    "lint": "npm run stylelint && npm run prettier && npm run eslint",
    "lint:check": "npm run stylelint:check && npm run prettier:check && npm run eslint:check",
    "prettier": "npm run prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "npm run prettier:base --check",
    "stylelint": "npm run stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@blueprintjs/core": "^3.54.0",
    "@date-io/date-fns": "^2.11.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@jupyterlab/application": "^3.4.1",
    "@jupyterlab/coreutils": "^5.0.0",
    "@jupyterlab/launcher": "^3.4.1",
    "@jupyterlab/notebook": "^3.4.1",
    "@jupyterlab/services": "^6.0.0",
    "@jupyterlab/terminal": "^3.4.1",
    "@lumino/widgets": "^1.32.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.7.0",
    "@mui/x-data-grid": "^5.1.0",
    "chart.js": "^3.7.0",
    "date-fns": "^2.27.0",
    "formik": "^2.2.9",
    "is-plain-object": "^5.0.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@jupyterlab/builder": "^3.0.0",
    "@types/node": "^15.12.5",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.15",
    "typescript": "~4.3.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "grader_labextension"
        }
      }
    },
    "extension": true,
    "outputDir": "grader_labextension/labextension"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ],
      "before-build-python": [
        "npm run clean:all"
      ]
    }
  }
}
